- var config = sails.config
- var staticFile = config.staticFile

doctype html
html#ng-app(ng-app='kingo')
  head
    title=config.appName

    meta(charset='utf-8')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    meta(http-equiv='X-UA-Compatible' content='IE=Edge,chrome=1')
    meta(name='keywords', content=config.keywords)
    meta(name='description', content=config.description)

    link(rel='shortcut icon' href=staticFile+'favicon.ico')
    link(rel='stylesheet' href='styles/style.css')

    script(src='http://cdn.staticfile.org/jquery/1.11.0/jquery.min.js')
    script(src='http://cdn.staticfile.org/twitter-bootstrap/3.1.1/js/bootstrap.min.js')

    // [if lt IE 9]
    script(src='http://cdn.staticfile.org/html5shiv/r29/html5.min.js')
    script(src='http://cdn.staticfile.org/respond.js/1.4.2/respond.min.js')
    script(src='http://cdn.staticfile.org/json5/0.3.0/json5.min.js')
    // [endif]
  body
    block body
    script.
